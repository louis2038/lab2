{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test-bubble",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/bubble.run",
            "args": ["10"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/src/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        },
    ]
}